{
  "name": "document-management-system",
  "version": "1.0.0",
  "description": "The system manages documents, users and user roles. Each document defines access rights; the document defines which roles can access it. Also, each document specifies the date it was published.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "open:client": "babel-node start-client.js",
    "open:server": "nodemon server.js --exec babel-node",
    "test": "npm run db:migrate && NODE_ENV=test npm run db:seed && NODE_ENV=test _mocha --compilers js:babel-core/register server/test/middleware/*.spec.js server/test/models/*.spec.js server/test/controllers/*.spec.js && npm run test:client",
    "test:watch": "npm run test -- -w",
    "test:client": "babel-node start-client-test.js",
    "db:migrate": "NODE_ENV=test sequelize db:migrate",
    "db:seed": "./node_modules/.bin/babel-node server/test/helpers/Seeder.js",
    "build": "rm -rf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./tests,./coverage --copy-files",
    "cover": "NODE_ENV=test npm run db:seed && NODE_ENV=test istanbul cover _mocha -- ./server/test/controllers/*.spec.js server/test/models/*.spec.js server/test/middleware/*.spec.js --compilers js:babel-core/register && npm run test:client",
    "coveralls": "npm run cover && cat ./client/coverage/lcov.info | coveralls"
  },
  "author": "Etanuwoma Jude",
  "license": "ISC",
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-loader": "6.2.4",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-react": "6.5.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.8.0",
    "chai-spies": "^0.7.1",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "compression": "1.6.1",
    "cross-env": "1.0.7",
    "css-loader": "^0.23.1",
    "enzyme": "2.2.0",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-watch": "2.1.11",
    "expect": "1.19.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.8.5",
    "istanbul": "^1.0.0-alpha.2",
    "jsdom": "8.5.0",
    "mocha": "2.4.5",
