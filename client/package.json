{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3001/",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-polyfill": "^6.23.0",
    "history": "^4.6.1",
    "jquery": "^3.2.1",
    "jwt-decode": "^2.2.0",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.5",
    "react-router": "^2.4.0",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-scroll-pagination": "^0.1.9",
    "react-swal": "^2.0.0",
    "react-tinymce": "^0.5.1",
    "redux": "^3.6.0",
    "redux-immutable-state-invariant": "^2.0.0",
    "redux-thunk": "^2.2.0",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "node-sass-chokidar": "0.0.2",
    "npm-run-all": "^4.0.2",
    "react-scripts": "1.0.7"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src/assets/styles/c --include-path ./node_modules src/assets/styles/ -o src/assets/styles/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src/assets/styles/ --include-path ./node_modules src/assets/styles/ -o src/assets/styles/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
