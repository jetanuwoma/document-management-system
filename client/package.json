    "babel-polyfill": "^6.23.0",
    "classnames": "^2.2.5",
    "enzyme": "^2.9.0",
    "expect": "^1.20.2",
    "expect-enzyme": "^1.0.0",
    "history": "^4.6.1",
    "jquery": "^3.2.1",
    "jwt-decode": "^2.2.0",
    "keymirror": "^0.1.1",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "rc-pagination": "^1.9.7",
    "react": "^15.6.1",
    "react-dom": "^15.5.4",
    "react-notification-system": "^0.2.14",
    "react-notification-system-redux": "^1.1.3",
    "react-redux": "^5.0.5",
    "react-router": "^2.4.0",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-swal": "^2.0.0",
    "react-test-renderer": "^15.6.1",
    "react-tinymce": "^0.5.1",
    "react-tinymce-input": "^1.2.1",
    "react-validation": "^2.10.9",
